def estrictamenteCreciente(arr):
    if not arr or len(arr) == 1:
        return 0
    roturas = 0
    for i in range(len(arr) - 1):
        if arr[i] >= arr[i+1]:
            roturas += 1
    return roturas


print(f"Resultado para [1, 3, 5, 2, 4, 6, 8, 8, 9, 10]: {estrictamenteCreciente([1, 3, 5, 2, 4, 6, 8, 8, 9, 10])}")
