def busquedaB(a, valor):
    arriba = 0
    abajo = len(a) - 1
    while arriba <= abajo:
        medio = (arriba + abajo) // 2
        if a[medio] == valor:
            return medio
        if a[medio] < valor:
            arriba = medio + 1
        else:
            abajo = medio - 1
    return "No encontrado"


ordenados = [1, 3, 5, 7, 9]
print("Resultado de búsqueda binaria (buscando 5):", busquedaB(ordenados, 5)) 
print("Resultado de búsqueda binaria (buscando 8):", busquedaB(ordenados, 8))  
